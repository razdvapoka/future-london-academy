@import '../../style/utils.styl'

.section
  display: flex
  margin-top: pxToRem(73)

.column
  flex: 1
  display: flex
  flex-direction: column
  justify-content: center

.preText
  position: absolute
  top: - pxToRem(45)
  left: pxToRem(8)

.list
  position: relative
  list-style-type: none

.item,
.itemSelected
  cursor: pointer
  color: $GREY
  transition: color 0.3s ease

.itemSelected
  color: $BLACK
